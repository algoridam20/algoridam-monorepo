{
    "name": "time spend",
    "description": "personal time spend analytics on chrome!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "service-worker.js"
    },
    "permissions": [
        "storage",
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_popup": "view/index.html",
        "default_icon": {
            "16": "/images/line-chart.png",
            "32": "/images/line-chart.png",
            "48": "/images/line-chart.png",
            "128": "/images/line-chart.png"
        }
    },
    "icons": {
        "16": "/images/line-chart.png",
        "32": "/images/line-chart.png",
        "48": "/images/line-chart.png",
        "128": "/images/line-chart.png"
    }
}